@model CBIB.Views.ViewModels.HomeIndexViewModel

@{
    ViewData["Title"] = "Details";
}

<h3>List of research papers </h3>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Journals<br />
        </dt>
        <dd>
            <table class="table">
                <tr>
                    <th>Title</th>
                    <th>Year</th>
                </tr>
                @foreach (var item in Model.dict)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Key.Title)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Key.Year)
                        </td>
                    </tr>
                }
            </table>
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
